{
    "template": "logstash-*",
    "settings": {
        "number_of_shards": 4,
        "number_of_replicas": 0,
        "index": {
            "codec": "best_compression",
            "query": { "default_field": "message" },
            "refresh_interval": "30s",
            "translog": { "sync_interval": "30s", "durability": "async" }
        }
    },
    "mappings": {
        "_default_": {
            "_all": { "enabled": false },
            "dynamic_templates": [
                {
                    "message_field": {
                        "path_match": "message",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "text",
                            "norms": false
                        }
                    }
                },
                {
                    "string_fields": {
                        "match": "*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "keyword",
                            "norms": false
                        }
                    }
                }
            ],
            "properties":{
                "@timestamp":{"type":"date","include_in_all":false},
                "@version":{"type":"keyword","include_in_all":false},
                "app":{"properties":{"pause":{"properties":{"threads_time":{"type":"float"},"total_time":{"type":"float"}}}}}
                "beat":{"properties":{"hostname":{"type":"keyword"},"name":{"type":"keyword"},"version":{"type":"keyword"}}},
                "cp":{"properties":{"fsync_time":{"type":"long"},"id":{"type":"keyword"},"lock_hold_time":{"type":"long"},"lock_wait_time":{"type":"long"},"mark_begin_time":{"type":"long"},"mark_end_time":{"type":"long"},"pages":{"type":"long"},"pages_write_time":{"type":"long"},"reason":{"type":"keyword"},"throughput":{"type":"long"},"total_time":{"type":"long"}}},
                "cpu":{"properties":{"avg":{"type":"float"},"current":{"type":"float"},"gc":{"type":"float"}}},
                "csv":{"properties":{"header":{"type":"object"}}},
                "dstat":{"properties":{"async":{"properties":{"#aio":{"type":"float"}}},"dsk/total":{"properties":{"read":{"type":"float"},"writ":{"type":"float"}}},"interrupts":{"properties":{"280":{"type":"float"},"281":{"type":"float"},"282":{"type":"float"},"283":{"type":"float"},"284":{"type":"float"}}},"io/total":{"properties":{"read":{"type":"float"},"writ":{"type":"float"}}},"load_avg":{"properties":{"15m":{"type":"float"},"1m":{"type":"float"},"5m":{"type":"float"}}},"memory_usage":{"properties":{"buff":{"type":"float"},"cach":{"type":"float"},"free":{"type":"float"},"used":{"type":"float"}}},"net/total":{"properties":{"recv":{"type":"float"},"send":{"type":"float"}}},"paging":{"properties":{"in":{"type":"float"},"out":{"type":"float"}}},"procs":{"properties":{"blk":{"type":"float"},"new":{"type":"float"},"run":{"type":"float"}}},"swap":{"properties":{"free":{"type":"float"},"used":{"type":"float"}}},"system":{"properties":{"csw":{"type":"float"},"int":{"type":"float"}}},"total_cpu_usage":{"properties":{"hiq":{"type":"float"},"idl":{"type":"float"},"siq":{"type":"float"},"sys":{"type":"float"},"usr":{"type":"float"},"wai":{"type":"float"}}},"virtual_memory":{"properties":{"alloc":{"type":"float"},"free":{"type":"float"},"majpf":{"type":"float"},"minpf":{"type":"float"}}}}},
                "emitter":{"type":"keyword"},
                "gc":{"properties":{"eden":{"properties":{"comm":{"properties":{"1":{"type":"long"},"2":{"type":"long"}}},"comm_m":{"type":"object"},"used":{"properties":{"1":{"type":"long"},"2":{"type":"long"}}},"used_m":{"type":"object"}}},"flags":{"type":"keyword"},"heap":{"properties":{"comm":{"properties":{"1":{"type":"long"},"2":{"type":"long"}}},"comm_m":{"type":"object"},"used":{"properties":{"1":{"type":"long"},"2":{"type":"long"}}},"used_m":{"type":"object"}}},"mem":{"properties":{"page_size":{"type":"keyword"},"phys":{"properties":{"free":{"type":"keyword"},"total":{"type":"keyword"}}},"swap":{"properties":{"free":{"type":"keyword"},"total":{"type":"keyword"}}}}},"meta":{"properties":{"comm":{"type":"long"},"used":{"properties":{"1":{"type":"long"},"2":{"type":"long"}}},"used_m":{"type":"object"}}},"msg":{"properties":{"type":{"type":"keyword"}}},"pause":{"properties":{"time":{"properties":{"real":{"type":"float"},"sys":{"type":"float"},"user":{"type":"float"}}},"type":{"type":"keyword"}}},"surv":{"properties":{"1":{"type":"long"},"2":{"type":"long"}}},"surv_m":{"type":"object"}}},
                "geoip":{"dynamic":"true","properties":{"ip":{"type":"ip"},"latitude":{"type":"half_float"},"location":{"type":"geo_point"},"longitude":{"type":"half_float"}}},
                "grid":{"properties":{"host":{"type":"keyword"},"name":{"type":"keyword"}}},
                "heap":{"properties":{"comm_mb":{"type":"long"},"free":{"type":"float"},"used_mb":{"type":"long"}}},
                "host":{"type":"keyword"},
                "input_type":{"type":"keyword"},
                "level":{"type":"keyword"},
                "mem":{"properties":{"pages":{"type":"long"}}},
                "message":{"type":"text","norms":false},
                "message_kind":{"type":"keyword"},
                "message_type":{"type":"keyword"},
                "node":{"properties":{"name":{"type":"keyword"},"short_id":{"type":"keyword"},"uptime":{"type":"keyword"}}},
                "non_heap":{"properties":{"free":{"type":"float"},"used_mb":{"type":"long"}}},
                "offset":{"type":"long"},
                "source":{"type":"keyword"},
                "src":{"properties":{"file":{"type":"keyword"},"order":{"type":"long"}}},
                "tags":{"type":"keyword"},
                "thread_pool":{"properties":{"pub":{"properties":{"active":{"type":"long"},"idle":{"type":"long"},"queue_size":{"type":"long"}}},"sys":{"properties":{"active":{"type":"long"},"idle":{"type":"long"},"queue_size":{"type":"long"}}}}},
                "top":{"properties":{"cpu_count":{"type":"long"},"hosts":{"type":"long"},"nodes":{"type":"long"},"snap":{"properties":{"clients":{"type":"long"},"cpu_count":{"type":"long"},"heap_size_gb":{"type":"float"},"servers":{"type":"long"},"ver":{"type":"long"}}}}},
                "type":{"type":"keyword"},
                "uptime":{"type":"float"},
                "wal":{"properties":{"force_flush":{"type":"keyword"},"hist_size":{"type":"long"},"idx":{"type":"long"},"len":{"type":"long"},"off":{"type":"long"},"segs_cleared":{"type":"long"}}}
            }
        }
    }
}