{
    "template": "logstash-*",
    "settings": {
        "number_of_shards": 4,
        "number_of_replicas": 0,
        "index": {
            "codec": "best_compression",
            "query": { "default_field": "message" },
            "refresh_interval": "30s",
            "translog": { "sync_interval": "30s", "durability": "async" }
        }
    },
    "mappings": {
        "_default_": {
            "_all": { "enabled": false },
            "dynamic_templates": [
                {
                    "message_field": {
                        "path_match": "message",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "text",
                            "norms": false
                        }
                    }
                },
                {
                    "string_fields": {
                        "match": "*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "keyword",
                            "norms": false
                        }
                    }
                }
            ],
			"properties":{
				"@timestamp":{"type":"date","include_in_all":false},
				"@version":{"type":"keyword","include_in_all":false},
				"beat":{
					"properties":{
						"hostname":{"type":"keyword"},
						"name":{"type":"keyword"},
						"version":{"type":"keyword"}
					}
				},
				"cp":{
					"properties":{
						"fsync_time":{"type":"long"},
						"id":{"type":"keyword"},
						"mark_begin_time":{"type":"long"},
						"mark_end_time":{"type":"long"},
						"pages":{"type":"long"},
						"pages_write_time":{"type":"long"},
						"total_time":{"type":"long"}
					}
				},
				"emitter":{"type":"keyword"},
				"gc":{
					"properties":{
						"eden":{
							"properties":{
								"comm":{
									"properties":{
										"1":{"type":"long"},
										"2":{"type":"long"}
									}
								},
								"comm_m":{"type":"object"},
								"used":{
									"properties":{
										"1":{"type":"long"},
										"2":{"type":"long"}
									}
								},
								"used_m":{"type":"object"}
							}
						},
						"heap":{
							"properties":{
								"comm":{
									"properties":{
										"1":{"type":"long"},
										"2":{"type":"long"}
									}
								},
								"comm_m":{"type":"object"},
								"used":{
									"properties":{
										"1":{"type":"long"},
										"2":{"type":"long"}
									}
								},
								"used_m":{"type":"object"}
							}
						},
						"meta":{
							"properties":{
								"comm":{"type":"long"},
								"used":{
									"properties":{
										"1":{"type":"long"},
										"2":{"type":"long"}
									}
								},
								"used_m":{"type":"object"}
							}
						},
						"msg":{
							"properties":{
								"type":{"type":"keyword"}
							}
						},
						"pause":{
							"properties":{
								"time":{
									"properties":{
										"real":{"type":"float"},
										"sys":{"type":"float"},
										"user":{"type":"float"}
									}
								},
								"type":{"type":"keyword"}
							}
						},
						"surv":{
							"properties":{
								"1":{"type":"long"},
								"2":{"type":"long"}
							}
						},
						"surv_m":{"type":"object"}
					}
				},
				"geoip":{
					"dynamic":"true",
					"properties":{
						"ip":{"type":"ip"},
						"latitude":{"type":"half_float"},
						"location":{"type":"geo_point"},
						"longitude":{"type":"half_float"}
					}
				},
				"grid":{
					"properties":{
						"host":{"type":"keyword"},
						"name":{"type":"keyword"}
					}
				},
				"host":{"type":"keyword"},
				"input_type":{"type":"keyword"},
				"level":{"type":"keyword"},
				"message":{"type":"text","norms":false},
				"message_kind":{"type":"keyword"},
				"message_type":{"type":"keyword"},
				"offset":{"type":"long"},
				"source":{"type":"keyword"},
				"src":{
					"properties":{
						"file":{"type":"keyword"}
					}
				},
				"tags":{"type":"keyword"},
				"thread":{
					"properties":{
						"name":{"type":"keyword"},
						"number":{"type":"long"},
						"prefix":{"type":"keyword"}
					}
				},
				"top":{
					"properties":{
						"snap":{
							"properties":{
								"clients":{"type":"long"},
								"cpu_count":{"type":"long"},
								"heap_size_gb":{"type":"float"},
								"servers":{"type":"long"},
								"ver":{"type":"long"}
							}
						}
					}
				},
				"type":{"type":"keyword"},
				"uptime":{"type":"float"},
				"wal":{
					"properties":{
						"idx":{"type":"long"},
						"len":{"type":"long"},
						"off":{"type":"long"},
						"segs_cleared":{"type":"long"}
					}
				}
			}
        }
    }
}